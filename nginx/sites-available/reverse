# server.finley.estate
server {

        listen 80 default_server;
        listen [::]:80 default_server;
    
        server_name server.finley.estate 173.208.201.178;
	return 301 https://$server_name$request_uri;
}

server {
 # SSL configuration

 listen 443 ssl http2 default_server;
 listen [::]:443 ssl http2 default_server;
 include /etc/nginx/snippets/strong-ssl.conf;
 ssl_certificate /etc/letsencrypt/live/server.finley.estate/fullchain.pem;
 ssl_certificate_key /etc/letsencrypt/live/server.finley.estate/privkey.pem;
 include /etc/nginx/snippets/finflix.conf;
}

